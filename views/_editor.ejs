<div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="nome">Nome</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="nome" value="<%= model.nome %>" />
    </div>
  </div>
  
  <div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="email">Email</label>
    <div class="col-sm-7">
      <input class="form-control" name="email" value="<%= model.email %>" />
    </div>
  </div>
  
  <div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="telefone">telefone</label>
    <div class="col-sm-10">
        <input class="form-control" name="telefone" value="<%= model.telefone %>" />
    </div>
  </div>