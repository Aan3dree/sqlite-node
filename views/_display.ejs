<div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="nome">Nome</label>
    <div class="col-sm-8">
      <input readonly class="form-control" id="nome" value="<%= model.nome %>" />
    </div>
  </div>
  
  <div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="email">Email</label>
    <div class="col-sm-7">
      <input readonly class="form-control" id="email" value="<%= model.email %>" />
    </div>
  </div>
  
  <div class="form-group row mb-3">
    <label class="col-form-label col-sm-2" for="telefone">Telefone</label>
    <div class="col-sm-7">
      <input readonly class="form-control" id="telefone" value="<%= model.telefone %>" />
    </div>
  </div>